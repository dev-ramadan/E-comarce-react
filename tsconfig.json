{
  "compilerOptions": {
    "moduleResolution": "bundler", 
    "jsx": "react-jsx",
    "target": "ESNext",
    "module": "ESNext",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["vite/client"]
  }
}


